{
    "name": "tarsoft/laravel-auto",
    "description": "A package to auto-generate CRUD for Laravel models",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "illuminate/support": "^8.0|^9.0|^10.0"    
    },
    "autoload": {
        "psr-4": {
            "Tarsoft\\LaravelAuto\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tarsoft\\LaravelAuto\\LaravelAutoServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4"
    }
}
